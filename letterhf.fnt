% Font fuer letterhf-Notation: C C#/Db D D#/Eb E F F#/Gb G G#/Ab A A#/B H c

% norm: nothing :)
	/norm
	{
	} def

% Font fuer Cypher-Notation

	 /myline {
	   % Ziel: 0,w,-dx,-dy,0,-w,dx,dy, Ausgangslage: dx,dy,w

	   dup neg 0 exch 5 2 roll % 0,-w,dx,dy,w
	   3 1 roll                % 0,-w,w,dx,dy
	   dup neg 6 1 roll        % -dy,0,-w,w,dx,dy
	   exch dup neg 7 1 roll   % -dx,-dy,0,-w,w,dy,dx
	   exch 3 2 roll           % -dx,-dy,0,-w,dx,dy,w
	   7 1 roll                % w,-dx,-dy,0,-w,dx,dy
	   0 8 1 roll              % 0,w,-dx,-dy,0,-w,dx,dy

	   gsave
	   currentpoint newpath moveto
	   rlineto
	   rlineto
	   rlineto
	   rlineto
	   fill
	   grestore
	 } def

/slw 1 def % sharp line width 0.5
/shlw 1.5 def % sharp hor line width 0.5
% Kreuz
	/sharp {
	   gsave
	   dup stringwidth pop 2 div
	   2.25 rmoveto

	   4 2 shlw myline
	   0 -3.5 rmoveto
	   4 2 shlw myline
	   currentpoint
	   newpath
	   moveto
	   slw setlinewidth
	   1 5.75  rmoveto
	   0 -8.5 rlineto
	   2 1  rmoveto
	   0 8.5  rlineto

	   stroke

	   grestore
	} def

/blw 1 def % bee line width

% Beee: wird wirklich auch noch immer wieder ben”tigt
	/flat {
	   gsave
	   1 setlinejoin
	   blw setlinewidth
	   dup stringwidth pop 2 div 0.75 add
	   5.5 rmoveto
	   0 -9 rlineto
%           2 2.5 4 5 0 5 rcurveto
	   2.5 2.5 4.5 5 0 5 rcurveto

	   stroke

	   grestore

	} def


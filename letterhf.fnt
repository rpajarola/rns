% dot:
	/sym_dot
	{
	  currentpoint
	  currentpoint
	   newpath 1 0 360 newpath arc fill
	  moveto
	} def

% Text Symbol drucken
% stack: <string>
	/sym_print
	{
	  dup
	  stringwidth
	  exch 2 div neg % x-Wert
	  exch pop -4  % y-Wert
	  rmoveto
	  show
	} def

	/SymBlank % " "
